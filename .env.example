# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to git

# Supabase Configuration
REACT_APP_SUPABASE_URL=your_supabase_url_here
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# API Configuration
REACT_APP_API_URL=https://qurancompare.onrender.com

# Voice API Configuration
# Set this to your Render deployment URL once deployed
REACT_APP_VOICE_API_URL=https://your-voice-api.onrender.com

# Stripe Configuration (if needed)
STRIPE_DEBATER_PRICE_ID=your_stripe_price_id_here

# RevenueCat Configuration (for mobile payments - optional)
REACT_APP_REVENUECAT_IOS_KEY=your_revenuecat_ios_key_here
REACT_APP_REVENUECAT_ANDROID_KEY=your_revenuecat_android_key_here

# OpenAI Configuration (if needed)
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here